    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --max-warnings=0 --no-ignore"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
